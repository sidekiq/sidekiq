---
<% if @verbose %>
:verbose: <%= @verbose %>
<% end %>
<% if @environment %>
:environment: <%= @environment %>
<% end %>
<% if @require %>
:require: <%= @require %>
<% end %>
<% if @pidfile %>
:pidfile: <%= @pidfile %>
<% end %>
<% if @concurrency %>
:concurrency: <%= @concurrency %>
<% end %>
<% if @queues %>
:queues:
  <% @queues.each do |name, priority| %>
  - [<%= name %>, <%= priority %>]
  <% end %>
<% end %>
